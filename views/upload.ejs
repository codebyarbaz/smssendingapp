<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title><%= title %></title>
    <link
      href="https://fonts.googleapis.com/css?family=Roboto"
      rel="stylesheet"
    />
    <link rel="stylesheet" href="/css/common.css" />
    <link rel="stylesheet" href="/css/nav.css" />
    <link rel="stylesheet" href="/css/upload.css" />
    <script src="/bower_components/angular/angular.min.js"></script>
  </head>
  <body ng-app="myApp">
    <%- include ./partials/header %>

    <div id="fileUpload" ng-controller="uploadFileController">
      <div class="formWrapper">
        <form action="/upload" method="post" enctype="multipart/form-data">
          <table>
            <tr>
              <td><input type="file" name="image_1" class="image" /></td>
            </tr>
            <tr>
              <td><input type="file" name="image_2" class="image" /></td>
            </tr>
            <tr>
              <td><input type="file" name="image_3" class="image" /></td>
            </tr>
            <tr>
              <td><input type="file" name="image_4" class="image" /></td>
            </tr>
            <tr>
              <td>
                <button class="btn btn_submit" type="submit">Submit</button>
              </td>
            </tr>
          </table>
        </form>
      </div>
    </div>
  </body>

  <script src="/js/app.js"></script>
  <script src="/js/toggleMenu.js"></script>
  <script src="/js/controllers/uploadFileController.js"></script>
  <script src="/js/models/uploadFileFactory.js"></script>
</html>
